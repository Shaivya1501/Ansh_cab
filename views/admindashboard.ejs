<!DOCTYPE html>
<html lang="en">
  <head>
    <meta charset="UTF-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    <title>Admin Panel</title>
    <link
      href="https://cdn.jsdelivr.net/npm/bootstrap@5.3.3/dist/css/bootstrap.min.css"
      rel="stylesheet"
      integrity="sha384-QWTKZyjpPEjISv5WaRU9OFeRpok6YctnYmDr5pNlyT2bRjXh0JMhjY6hW+ALEwIH"
      crossorigin="anonymous"
    />
  </head>
  <body>
    <% if (successMessage) { %>
    <div
      class="flash-message success"
      style="font-size: 18px; color: orangered; margin: 10px 0"
    >
      <%= successMessage %>
    </div>
    <% } %>

    <!-- Display error flash message -->
    <% if (errorMessage) { %>
    <div class="flash-message error" style="font-size: 18px; color: orangered">
      <%= errorMessage %>
    </div>
    <% } %>

    <!-- Table 2 - Booking Section  -->
    <section class="py-3 py-md-5">
      <div class="container">
        <div class="row justify-content-center">
          <div class="col-12 col-lg-9 col-xl-8">
            <div class="card widget-card border-light shadow-sm">
              <div class="card-body p-4">
                <h2 class="card-title widget-card-title mb-4 fw-bold">
                  Customer's Booking's
                </h2>
                <div class="table-responsive">
                  <table
                    class="table table-borderless bsb-table-xl text-nowrap align-middle m-0"
                  >
                    <thead>
                      <tr>
                        <th class="fw-bold h4">Name</th>
                        <th class="fw-bold h4">Taxitype</th>
                        <th class="fw-bold h4">Email</th>
                        <th class="fw-bold h4">Pickup-Location</th>
                        <th class="fw-bold h4">Drop-Location</th>
                        <th class="fw-bold h4">Contact</th>
                        <th class="fw-bold h4">Date</th>
                        <th class="fw-bold h4">Call</th>
                        <th class="fw-bold h4">Confirm Booking</th>
                        <th class="fw-bold h4">Reject Booking</th>
                      </tr>
                    </thead>
                    <tbody>
                      <%booking.forEach(function(elem){%>
                      <tr>
                        <td>
                          <div class="d-flex align-items-center">
                            <span
                              class="fs-6 bsb-w-35 bsb-h-35 text-bg-primary rounded-circle d-flex align-items-center justify-content-center me-2"
                            >
                              <i class="bi bi-twitter-x"></i>
                            </span>
                            <div>
                              <h6 class="m-0"><%=elem.Name%></h6>
                            </div>
                          </div>
                        </td>
                        <td>
                          <h6 class="mb-1"><%=elem.taxitype%></h6>
                        </td>
                        <td>
                          <h6 class="mb-1"><%=elem.email%></h6>
                        </td>
                        <td>
                          <h6 class="mb-1"><%=elem.pickuplocation%></h6>
                        </td>
                        <td>
                          <h6 class="mb-1"><%=elem.droplocation%></h6>
                        </td>
                        <td>
                          <h6 class="mb-1"><%=elem.contact%></h6>
                        </td>
                        <td>
                          <% let dateString = new Date(elem.date).toLocaleDateString(); %>
                          <h6 class="mb-1"><%= dateString %></h6>
                        </td> 
                        <td >
                          <a  href="tel:<%=elem.contact%>"
                            ><span class="badge bg-warning bsb-w-85 text-dark"
                              >Call Customer</span
                            ></a
                          >
                        </td>
                        <td>
                          <%if(elem.bookingstatus == false ){%>
                            <a style="margin-left: 30px;" href="/confirmbooking/<%=elem._id%>"
                              ><span class="badge bg-primary bsb-w-85 text-dark"
                                >Confirm Booking</span
                              ></a
                            >
                          <%}else{%>
                            <a style="margin-left: 30px;" href=""
                              ><span class="badge bg-success bsb-w-85 text-dark"
                                >Booking Confirmed</span
                              ></a
                            >
                          <%}%>
                          
                        </td>
                        <td>
                          <a style="margin-left: 30px;" href="/rejectbooking/<%=elem._id%>"
                            ><span class="badge bg-danger bsb-w-85 text-dark"
                              >Reject Booking</span
                            ></a
                          >
                        </td>
                      </tr>
                      <%})%>
                    </tbody>
                    <hr />
                  </table>
                  <hr />
                </div>
              </div>
            </div>
          </div>
        </div>
      </div>
    </section>
    <hr />

    <!-- Table 2 - Query Section  -->
    <section class="py-3 py-md-5">
      <div class="container">
        <div class="row justify-content-center">
          <div class="col-12 col-lg-9 col-xl-8">
            <div class="card widget-card border-light shadow-sm">
              <div class="card-body p-4">
                <h2 class="card-title widget-card-title mb-4 fw-bold">
                  Customer's Queries
                </h2>
                <div class="table-responsive">
                  <table
                    class="table table-borderless bsb-table-xl text-nowrap align-middle m-0"
                  >
                    <thead>
                      <tr>
                        <th class="fw-bold h4">Name</th>
                        <th class="fw-bold h4">Mobile Number</th>
                        <th class="fw-bold h4">Query</th>
                        <th class="fw-bold h4">Status</th>
                        <th class="fw-bold h4">Delete Query</th>
                      </tr>
                    </thead>
                    <tbody>
                      <%queries.forEach(function(elem){%>
                      <tr>
                        <td>
                          <div class="d-flex align-items-center">
                            <span
                              class="fs-6 bsb-w-35 bsb-h-35 text-bg-primary rounded-circle d-flex align-items-center justify-content-center me-2"
                            >
                              <i class="bi bi-twitter-x"></i>
                            </span>
                            <div>
                              <h6 class="m-0"><%=elem.queriename%></h6>
                            </div>
                          </div>
                        </td>
                        <td>
                          <h6 class="mb-1"><%=elem.Phone%></h6>
                        </td>
                        <td>
                          <h6 class="mb-1"><%=elem.query%></h6>
                        </td>
                        <td>
                          <a href="tel:<%=elem.Phone%>"
                            ><span class="badge bg-success bsb-w-85"
                              >Call Customer</span
                            ></a
                          >
                        </td>
                        <td>
                          <a href="/delete/query/<%=elem._id%>"
                            ><span class="badge bg-danger bsb-w-85"
                              >Delete <%=elem.queriename%>'s Query</span
                            ></a
                          >
                        </td>
                      </tr>
                      <%})%>
                    </tbody>
                    <hr />
                  </table>
                  <hr />
                </div>
              </div>
            </div>
          </div>
        </div>
      </div>
    </section>
    <hr />
