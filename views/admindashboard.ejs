                    <div class="modal-content">
                      <div class="modal-header">
                        <h1 class="modal-title fs-5" id="exampleModalLabel">
                          Update Review from here
                        </h1>
                        <button
                          type="button"
                          class="btn-close"
                          data-bs-dismiss="modal"
                          aria-label="Close"
                        ></button>
                      </div>
                      <div class="modal-body fw-bold">
                        <form
                          action="/updatereview/<%=elem._id%>"
                          method="post"
                          enctype="multipart/form-data"
                        >
                          <div class="mb-3">
                            <label for="exampleInputEmail1" class="form-label"
                              >Reviewer name</label
                            >
                            <input
                              type="text"
                              class="form-control"
                              id="exampleInputEmail1"
                              aria-describedby="emailHelp"
                              name="reviewname"
                            />
                            <!-- <div id="emailHelp" class="form-text">We'll never share your email with anyone else.</div> -->
                          </div>
                          <div class="mb-3">
                            <label for="exampleInputEmail1" class="form-label"
                              >Review</label
                            >
                            <input
                              type="text"
                              class="form-control"
                              id="exampleInputEmail1"
                              aria-describedby="emailHelp"
                              name="reviewtype"
                            />
                          </div>

                          <button type="submit" class="btn btn-primary">
                            Submit
                          </button>
                        </form>
                      </div>
                    </div>
                  </div>
                </div>
                <a href="/review/delete/<%=elem._id%>"
                  ><button type="button" class="btn btn-danger mx-3 mb-3">
                    Delete Review
                  </button></a
                >
              </div>
            </div>
          </div>
          <%})%>
        </div>
      </div>
    </section>

    <!-- modal for add service  -->
    <div
      class="modal fade"
      id="exampleModal"
      tabindex="-1"
      aria-labelledby="exampleModalLabel"
      aria-hidden="true"
    >
