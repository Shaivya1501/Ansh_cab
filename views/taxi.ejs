<!doctype html>
<html lang="en">
  <head>
    <meta charset="utf-8">
    <meta name="viewport" content="width=device-width, initial-scale=1">
    <title>Ansh Cab Services</title>

   <!-- slider stylesheet -->
   <link rel="stylesheet" type="text/css" href="https://cdnjs.cloudflare.com/ajax/libs/OwlCarousel2/2.3.4/assets/owl.carousel.min.css" />  

  <!-- bootstrap core css -->
  <link rel="stylesheet" type="text/css" href="/stylesheets/bootstrap.css" />

  <!-- fonts style -->
  <link href="https://fonts.googleapis.com/css?family=Poppins:400,700&display=swap" rel="stylesheet">

  <!-- Custom styles for this template -->
  <link href="/stylesheets/style.css" rel="stylesheet" />
  <!-- responsive style -->
  <link href="/stylesheets/responsive.css" rel="stylesheet" />

    <link href="https://cdn.jsdelivr.net/npm/bootstrap@5.3.3/dist/css/bootstrap.min.css" rel="stylesheet" integrity="sha384-QWTKZyjpPEjISv5WaRU9OFeRpok6YctnYmDr5pNlyT2bRjXh0JMhjY6hW+ALEwIH" crossorigin="anonymous">
  </head>
  <body class="sub_page">

    <div class="hero_area">
        <!-- header section strats -->
        <header class="header_section">
          <div class="container-fluid">
            <nav class="navbar navbar-expand-lg custom_nav-container ">
              <a class="navbar-brand" href="/">
                <span>
                  Ansh Cab Services
                </span>
              </a>
              </a>
              <button class="navbar-toggler" type="button" data-toggle="collapse" data-target="#navbarSupportedContent" aria-controls="navbarSupportedContent" aria-expanded="false" aria-label="Toggle navigation">
                <span class="navbar-toggler-icon"></span>
              </button>
    
              <div class="collapse navbar-collapse" id="navbarSupportedContent">
                <div class="d-flex ml-auto flex-column flex-lg-row align-items-center">
                  <ul class="navbar-nav  ">
                    <li class="nav-item ">
                      <a class="nav-link" href="/">Home <span class="sr-only">(current)</span></a>
                    </li>
                    <li class="nav-item">
                      <a class="nav-link" href="/about"> About</a>
                    </li>
                    <li class="nav-item active">
                      <a class="nav-link" href="/service"> Services </a>
                    </li>
                    <li class="nav-item">
                      <a class="nav-link" href="/taxi"> Our Taxi</a>
                    </li>
                    <li class="nav-item">
                      <a class="nav-link" href="/contact">Contact Us</a>
                    </li>
                    <li class="nav-item">
                      <a class="nav-link" href="/adminlogin">Admin Login</a>
                    </li>
                  </ul>
                </div>
              </div>
            </nav>
          </div>
        </header>
        <!-- end header section -->
      </div>
       <!-- Team 2 - Bootstrap Brain Component -->
       <section class="py-3 py-md-5 py-xl-8">
        <div class="container">
          <div class="row justify-content-md-center">
            <div class="col-12 col-md-10 col-lg-8 col-xl-7 col-xxl-6">
              <h2 class="fs-6 text-dark mb-2 text-uppercase text-center fw-bold">
                Our Taxi
              </h2>
              <p class="h4 mb-4 mb-md-5 text-center">
                With Our Taxi, you can ride in comfort and style, reaching your
                destination safely and reliably every time.
              </p>
              <hr class="w-50 mx-auto mb-5 mb-xl-9 border-dark-subtle" />
            </div>
          </div>
        </div>
  
        <div class="container overflow-hidden">
          <div class="row gy-4 gy-lg-0">
            <%taxi.forEach(function(elem){%>
            <div class="col-12 col-lg-4">
              <div class="card border-0">
                <figure
                  class="card-img-top m-0 overflow-hidden bsb-overlay-hover"
                >
                  <a href="#!">
                    <img
                      class="img-fluid bsb-scale bsb-hover-scale-up"
                      loading="lazy"
                      src="<%=elem.taxiimage1%>"
                      alt=""
                    />
                  </a>
                  <figcaption>
                    <svg
                      xmlns="http://www.w3.org/2000/svg"
                      width="32"
                      height="32"
                      fill="currentColor"
                      class="bi bi-eye text-white bsb-hover-fadeInLeft"
                      viewBox="0 0 16 16"
                    >
                      <path
                        d="M16 8s-3-5.5-8-5.5S0 8 0 8s3 5.5 8 5.5S16 8 16 8zM1.173 8a13.133 13.133 0 0 1 1.66-2.043C4.12 4.668 5.88 3.5 8 3.5c2.12 0 3.879 1.168 5.168 2.457A13.133 13.133 0 0 1 14.828 8c-.058.087-.122.183-.195.288-.335.48-.83 1.12-1.465 1.755C11.879 11.332 10.119 12.5 8 12.5c-2.12 0-3.879-1.168-5.168-2.457A13.134 13.134 0 0 1 1.172 8z"
                      />
                      <path
                        d="M8 5.5a2.5 2.5 0 1 0 0 5 2.5 2.5 0 0 0 0-5zM4.5 8a3.5 3.5 0 1 1 7 0 3.5 3.5 0 0 1-7 0z"
                      />
                    </svg>
                    <h4 class="h6 text-white bsb-hover-fadeInRight mt-2">
                      Read More
                    </h4>
                  </figcaption>
                </figure>
                <div class="card-body border bg-warning p-4">
                  <h2
                    class="card-title h4 fw-bold mb-2"
                    style="font-weight: bold"
                  >
                    Name : <%=elem.taxiname%>
                  </h2>
                  <h5 class="card-text text-dark mb-2" style="font-weight: bold">
                    Taxi Type : <%=elem.taxitype%>
                  </h5>
                  <h5 class="card-text text-dark mb-2" style="font-weight: bold">
                    Taxi fare per km : <%=elem.taxifare%>
                  </h5>
                  <h5 class="card-text text-dark mb-2" style="font-weight: bold">
                    Sitting Capecity : <%=elem.sittingcapecity%>
                  </h5>
                  <p class="card-text text-secondary">
                    Car Discription : <%=elem.taxidescription%>
                  </p>
                </div>
                <div class="card-footer border border-top-0 bg-white p-4">
                  <ul
                    class="nav mb-0 bsb-nav-sep d-flex align-items-center justify-content-between"
                  >
                    <li class="nav-item text-secondary">
                      <a
                        class="nav-link link-secondary p-0 pe-3 d-inline-flex align-items-center"
                        href="/carprofile/<%=elem._id%>"
                      >
                        <svg
                          xmlns="http://www.w3.org/2000/svg"
                          width="18"
                          height="18"
                          fill="currentColor"
                          class="bi bi-lightbulb text-primary"
                          viewBox="0 0 16 16"
                        >
                          <path
                            d="M2 6a6 6 0 1 1 10.174 4.31c-.203.196-.359.4-.453.619l-.762 1.769A.5.5 0 0 1 10.5 13a.5.5 0 0 1 0 1 .5.5 0 0 1 0 1l-.224.447a1 1 0 0 1-.894.553H6.618a1 1 0 0 1-.894-.553L5.5 15a.5.5 0 0 1 0-1 .5.5 0 0 1 0-1 .5.5 0 0 1-.46-.302l-.761-1.77a1.964 1.964 0 0 0-.453-.618A5.984 5.984 0 0 1 2 6zm6-5a5 5 0 0 0-3.479 8.592c.263.254.514.564.676.941L5.83 12h4.342l.632-1.467c.162-.377.413-.687.676-.941A5 5 0 0 0 8 1z"
                          />
                        </svg>
                        <span class="ms-2 fs-6">View Taxi Images</span>
                      </a>
                    </li>
                    <li class="nav-item text-secondary">
                      <button
                        class="btn btn-success"
                        data-bs-toggle="modal"
                        data-bs-target="#exampleModal3"
                      >
                        Book Now
                      </button>
  
                      <!-- Modal -->
                      <div
                        class="modal fade"
                        id="exampleModal3"
                        tabindex="-1"
                        aria-labelledby="exampleModalLabel"
                        aria-hidden="true"
                      >
                        <div class="modal-dialog">
                          <div class="modal-content">
                            <div class="modal-header">
                              <h1
                                class="modal-title fs-5 text-dark fw-bold"
                                id="exampleModalLabel"
                              >
                                Book Your Taxi
                              </h1>
                              <button
                                type="button"
                                class="btn-close"
                                data-bs-dismiss="modal"
                                aria-label="Close"
                              ></button>
                            </div>
                            <div class="modal-body">
                              <form action="/booking" method="post">
                                <input
                                  type="text"
                                  placeholder="Name"
                                  name="custname"
                                  class="mb-2"
                                />
                                <input
                                  type="email"
                                  placeholder="Enter Your Email"
                                  name="email"
                                  class="mb-2"
                                />
  
                                <input
                                  list="browsers"
                                  name="cartype"
                                  placeholder="car name"
                                  value="<%=elem.taxiname%>"
                                  class="mb-2"
                                  readonly
                                />
                                <input
                                  type="date"
                                  placeholder="Pick Up Location"
                                  name="date"
                                  class="mb-2"
                                />
                                <input
                                  type="text"
                                  placeholder="Pick Up Location"
                                  name="pickloc"
                                  class="mb-2"
                                />
                                <input
                                  type="text"
                                  placeholder="Drop Location"
                                  name="droploc"
                                  class="mb-2"
                                />
                                <div class="btm_input">
                                  <input
                                    type="text"
                                    placeholder="Your Phone Number"
                                    name="number"
                                  />
                                  <button>Book Now</button>
                                </div>
                              </form>
                            </div>
                          </div>
                        </div>
                      </div>
                    </li>
                  </ul>
                </div>
              </div>
            </div>
            <%})%>
          </div>
        </div>
      </section>

        <!-- info section -->

  <section class="info_section layout_padding-top layout_padding2-bottom">
    <div class="container">
      <div class="box">
        <div class="info_form">
          <h4>
            Ansh Cab Services
          </h4>
        </div>
        <div class="info_links">
          <ul>
            <li class=" ">
              <a class="" href="/">Home <span class="sr-only">(current)</span></a>
            </li>
            <li class="">
              <a class="" href="/about"> About</a>
            </li>
            <li class="">
              <a class="" href="/service"> Services </a>
            </li>
            <li class="">
              <a class="" href="/taxi">Our Taxi</a>
            </li>
            <li class="">
              <a class="" href="/contact">Contact Us</a>
            </li>
            <li class="">
              <a class="" href="/adminlogin">Admin Login</a>
            </li>
          </ul>
        </div>
        <div class="info_social">
          <div>
            <a href="">
              <img src="images/fb.png" alt="">
            </a>
          </div>
          <div>
            <a href="">
              <img src="images/twitter.png" alt="">
            </a>
          </div>
          <div>
            <a href="">
              <img src="images/linkedin.png" alt="">
            </a>
          </div>
          <div>
            <a href="">
              <img src="images/instagram.png" alt="">
            </a>
          </div>
        </div>
      </div>
    </div>

  </section>

  <!-- end info section -->

  <script
  type="text/javascript"
  src="/javascripts/jquery-3.4.1.min.js"
></script>
<script type="text/javascript" src="/javascripts/bootstrap.js"></script>
<script
  type="text/javascript"
  src="https://cdnjs.cloudflare.com/ajax/libs/OwlCarousel2/2.3.4/owl.carousel.min.js"
></script>

<!-- owl carousel script -->
<script type="text/javascript">
  $(".owl-carousel").owlCarousel({
    loop: true,
    margin: 20,
    navText: [],
    autoplay: true,
    autoplayHoverPause: true,
    responsive: {
      0: {
        items: 1,
      },
      768: {
        items: 2,
      },
      1000: {
        items: 2,
      },
    },
  });
</script>
<!-- end owl carousel script -->
<script
  src="https://cdn.jsdelivr.net/npm/bootstrap@5.3.3/dist/js/bootstrap.bundle.min.js"
  integrity="sha384-YvpcrYf0tY3lHB60NNkmXc5s9fDVZLESaAA55NDzOxhy9GkcIdslK1eN7N6jIeHz"
  crossorigin="anonymous"
></script>
</body>
</html>